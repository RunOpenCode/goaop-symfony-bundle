{
    "name": "goaop/goaop-symfony-bundle",
    "description": "Integration bridge for Go! AOP framework",
    "type": "symfony-bundle",
    "require": {
        "goaop/framework": "^1.0|^2.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/console": "^2.6|^3.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "symfony/phpunit-bridge": "^3.3.4",
        "symfony/debug": "~2.7|~3.0",
        "matthiasnoback/symfony-dependency-injection-test": "~2.0",
        "doctrine/orm": "~2.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lisachenko Alexander",
            "email": "lisachenko.it@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Go\\Symfony\\GoAopBundle\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Go\\Symfony\\GoAopBundle\\Tests\\": "./tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
